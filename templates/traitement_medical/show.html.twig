<!DOCTYPE html>

<title>TraitementMedical</title>

{% block body %}
    <h1>TraitementMedical</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ traitement_medical.id }}</td>
            </tr>
            <tr>
                <th>NomMedicament</th>
                <td>{{ traitement_medical.nomMedicament }}</td>
            </tr>
            <tr>
                <th>Dosage</th>
                <td>{{ traitement_medical.dosage }}</td>
            </tr>
            <tr>
                <th>Frequence</th>
                <td>{{ traitement_medical.frequence }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ traitement_medical.dateDebut ? traitement_medical.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ traitement_medical.dateFin ? traitement_medical.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_traitement_medical_index') }}">back to list</a>

    <a href="{{ path('app_traitement_medical_edit', {'id': traitement_medical.id}) }}">edit</a>

    {{ include('traitement_medical/_delete_form.html.twig') }}
{% endblock %}
